<!DOCTYPE html>
<html>

<head>
    <title>
        @ViewBag.Title
    </title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <!--REQUIRED STYLE SHEETS-->
    <!-- BOOTSTRAP CORE STYLE CSS -->
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <!-- FONTAWESOME STYLE CSS -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!--ANIMATED FONTAWESOME STYLE CSS -->
    <link href="~/Content/font-awesome-animation.css" rel="stylesheet" />
    <!-- VEGAS STYLE CSS -->
    <link href="~/Content/jquery.vegas.min.css" rel="stylesheet" />
    <!-- SIDE MENU STYLE CSS -->
    <link href="~/Content/component.css" rel="stylesheet" />
    <!-- CUSTOM STYLE CSS -->
    <link href="~/Content/style.css" rel="stylesheet" />
    <!-- GOOGLE FONT -->
    <link href='~/Content/fonts.googleapis.com.css' rel='stylesheet' type='text/css'>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

</head>

<body>
    
    <div class="container body-content">
        

        @RenderBody()

        <!--FOOTER SECTION -->
        <div class="for-full-back color-bg-one" id="footer">
            &copy; @DateTime.Now.Year  www.palmreader.com | All Right Reserved | Terms | Policies | Licenses

        </div>

        <!-- END FOOTER SECTION -->
        <!-- JAVASCRIPT FILES PLACED AT THE BOTTOM TO REDUCE THE LOADING TIME  -->
        <!-- CORE JQUERY  -->
        <script src="~/Scripts/jquery-1.10.2.js"></script>
        <!-- BOOTSTRAP CORE SCRIPT   -->
        <script src="~/Scripts/bootstrap.js"></script>
        <!-- SIDE MENU SCRIPTS -->
        <script src="~/Scripts/modernizr.custom.js"></script>
        <script src="~/Scripts/classie.js"></script>
        <!-- VEGAS SLIDESHOW SCRIPTS -->
        <script src="~/Scripts/jquery.vegas.min.js"></script>
        <!-- CUSTOM SCRIPTS -->
        <script src="~/Scripts/custom.js"></script>
        <script src="~/Scripts/jquery.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/jquery-1.10.2.min.js"></script>
        <script src="~/Scripts/jquery-2.2.3.js"></script>

    </div>

</body>
</html>